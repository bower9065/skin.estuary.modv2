				<aspectratio scalediffuse="false">keep</aspectratio>
		<visible>Skin.HasSetting(EnableCharacter) + Skin.HasSetting(HolidayThemes) + Skin.HasSetting(Spring) + Window.IsVisible(Home) + [!Window.IsActive(MovieInformation) + !Window.IsActive(MusicInformation) + !Window.IsActive(PVRGuideInfo) + !Window.IsActive(PVRRecordingInfo) + !Window.IsActive(PictureInfo) + !Window.IsActive(AddonInformation)]</visible>	
		<animation effect="fade" start="0" end="100" time="400">WindowOpen</animation>
		<animation effect="fade" start="100" end="0" time="300">WindowClose</animation>
		</control>	
	</include>		
